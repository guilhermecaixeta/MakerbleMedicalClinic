<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Specialty</th>
      <th scope="col">Email</th>
    </tr>
  </thead>
  <tbody class="table-group-divider">
    <% @objects.each do |obj| %>
      <tr>
        <td scope="row">
          <%= obj.name %>
        </td>
        <td>
          <%= obj.specialty.name %>
        </td>
        <td>
          <%= obj.email %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
<div class="pag-bar mt-5 pt-5">
  <%== pagy_bootstrap_nav(@pagy) %>
</div>
